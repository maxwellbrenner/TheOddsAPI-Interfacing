    "# Functional as of 2024.11.03\n",
    "import pandas as pd\n",
    "\n",
    "# Dictionary of fighter name replacements\n",
    "name_replacements = {\n",
    "    \"Zach Reese\": \"Zachary Reese\",\n",
    "    \"Maheshate\": \"Hayisaer Maheshate\",\n",
    "    \"Mizuki\": \"Mizuki Inoue\",\n",
    "    \"Blood Diamond\": \"Mike Mathetha\"\n",
    "}\n",
    "\n",
    "# Function to replace fighter names and update the winner field if applicable\n",
    "def replace_fighter_name_and_update_winner(row):\n",
    "    \"\"\"\n",
    "    This function checks if the names of fighters need to be updated according to a predefined dictionary \n",
    "    of name replacements. If a name update is required, the function modifies the fighter's name in \n",
    "    'fighter_a_name' or 'fighter_b_name' and updates the 'winner' column if the winner matches the old name.\n",
    "\n",
    "    Parameters:\n",
    "    - row (pd.Series): A row of the DataFrame, containing information about a single fight, including \n",
    "                       fighter names and the winner.\n",
    "\n",
    "    Process:\n",
    "    - For 'fighter_a_name':\n",
    "        1. Retrieves the current name in 'fighter_a_name'.\n",
    "        2. Checks if a replacement name is available in the `name_replacements` dictionary.\n",
    "        3. If a replacement exists, updates 'fighter_a_name' and prints the change.\n",
    "        4. If 'winner' matches the old name, updates 'winner' to the new name.\n",
    "    - For 'fighter_b_name':\n",
    "        1. Retrieves the current name in 'fighter_b_name'.\n",
    "        2. Checks for a replacement name in `name_replacements`.\n",
    "        3. If a replacement is found, updates 'fighter_b_name' and prints the change.\n",
    "\n",
    "    Returns:\n",
    "    - pd.Series: The modified row with updated names and winner field (if applicable).\n",
    "\n",
    "    Example:\n",
    "    Given a row with:\n",
    "        - fighter_a_name: \"Maheshate\"\n",
    "        - fighter_b_name: \"John Doe\"\n",
    "        - winner: \"Maheshate\"\n",
    "    If \"Maheshate\" is found in `name_replacements` as \"Hayisaer Maheshate\", the function:\n",
    "        1. Updates 'fighter_a_name' to \"Hayisaer Maheshate\".\n",
    "        2. Changes 'winner' to \"Hayisaer Maheshate\".\n",
    "    \"\"\"\n",
    "\n",
    "    # Check if fighter_a_name needs to be updated\n",
    "    old_fighter_a = row['fighter_a_name']\n",
    "    new_fighter_a = name_replacements.get(old_fighter_a, old_fighter_a)\n",
    "    \n",
    "    if new_fighter_a != old_fighter_a:\n",
    "        print(f\"Replacing fighter_a_name: {old_fighter_a} -> {new_fighter_a}\")\n",
    "        row['fighter_a_name'] = new_fighter_a\n",
    "        # Update winner if necessary (not an 'NC' or 'Draw')\n",
    "        if row['winner'] == old_fighter_a:\n",
    "            print(f\"Updating winner from {old_fighter_a} to {new_fighter_a}\")\n",
    "            row['winner'] = new_fighter_a\n",
    "    \n",
    "    # Check if fighter_b_name needs to be updated\n",
    "    old_fighter_b = row['fighter_b_name']\n",
    "    new_fighter_b = name_replacements.get(old_fighter_b, old_fighter_b)\n",
    "    \n",
    "    if new_fighter_b != old_fighter_b:\n",
    "        print(f\"Replacing fighter_b_name: {old_fighter_b} -> {new_fighter_b}\")\n",
    "        row['fighter_b_name'] = new_fighter_b\n",
    "    \n",
    "    return row\n",
    "\n",
    "\n",
    "# Function to drop all columns except the specified ones\n",
    "def drop_columns(df):\n",
    "    \"\"\"\n",
    "    This function filters the DataFrame to retain only the specified columns, \n",
    "    removing any columns that are not in the `columns_to_keep` list. It is useful \n",
    "    for reducing the dataset to essential information for further analysis.\n",
    "\n",
    "    Parameters:\n",
    "    - df (pd.DataFrame): The input DataFrame with multiple columns.\n",
    "\n",
    "    Process:\n",
    "    - Defines a list, `columns_to_keep`, containing the names of columns to retain.\n",
    "    - Filters the DataFrame to include only these columns.\n",
    "    - Prints a message listing the columns that will be kept.\n",
    "\n",
    "    Returns:\n",
    "    - pd.DataFrame: The filtered DataFrame with only the specified columns.\n",
    "\n",
    "    Example:\n",
    "    If the DataFrame contains columns 'event_name', 'winner', 'fighter_a_name', 'round_of_victory', \n",
    "    'extra_column', and `columns_to_keep` specifies 'event_name', 'winner', and 'fighter_a_name', \n",
    "    the function returns a DataFrame with only 'event_name', 'winner', and 'fighter_a_name', dropping 'extra_column'.\n",
    "    \"\"\"\n",
    "    \n",
    "    # List of columns to keep\n",
    "    columns_to_keep = [\n",
    "        'event_name', 'event_date', 'event_location', 'winner', \n",
    "        'fighter_a_name', 'fighter_b_name', 'weightclass', 'method_of_victory', \n",
    "        'round_of_victory', 'time_of_victory', 'time_format', 'referee', \n",
    "        'title_fight', 'gender'\n",
    "    ]\n",
    "    \n",
    "    # Drop all columns except those in the columns_to_keep list\n",
    "    df_filtered = df[columns_to_keep]\n",
    "    \n",
    "    print(f\"Columns dropped. Keeping only the following columns:\\n{columns_to_keep}\")\n",
    "    return df_filtered\n",
    "\n",
    "\n",
    "# Main program\n",
    "if __name__ == \"__main__\":\n",
    "    file_path = "cleaned_event_masterlist.csv\"\n",
    "    \n",
    "    # Load the CSV file into a DataFrame\n",
    "    print(f\"Loading data from {file_path}...\")\n",
    "    df = pd.read_csv(file_path, low_memory=False)\n",
    "    print(\"Dataset loaded.\")\n",
    "\n",
    "    # Step 1: Replace fighter names and update the winner\n",
    "    print(\"Starting fighter name replacement...\")\n",
    "    df_cleaned_names = df.apply(replace_fighter_name_and_update_winner, axis=1)\n",
    "    print(\"Fighter name replacement complete.\")\n",
    "\n",
    "    # Step 2: Drop unwanted columns\n",
    "    print(\"Dropping unwanted columns...\")\n",
    "    df_final_cleaned = drop_columns(df_cleaned_names)\n",
    "    print(\"Unwanted columns dropped.\")\n",
    "\n",
    "    # Step 3: Save the cleaned DataFrame to a CSV file\n",
    "    output_file_path = "preprocessed_event_masterlist.csv\"\n",
    "    df_final_cleaned.to_csv(output_file_path, index=False)\n",
    "    print(f\"Cleaned data saved to: {output_file_path}\")\n"
   ]
